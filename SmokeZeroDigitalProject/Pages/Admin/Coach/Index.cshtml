							</div>
							<div class="user-text">
								<h6>Ryan Taylor</h6>
								<p class="text-muted mb-0">Administrator</p>
							</div>
						</div>
						<a class="dropdown-item" href="profile.html">My Profile</a>
						<a class="dropdown-item" href="settings.html">Settings</a>
						<a class="dropdown-item" href="login.html">Logout</a>
					</div>
				</li>
				<!-- /User Menu -->

			</ul>
			<!-- /Header Right Menu -->

		</div>
		<!-- /Header -->
		<!-- Sidebar -->
		<div class="sidebar" id="sidebar">
			<div class="sidebar-inner slimscroll">
				<div id="sidebar-menu" class="sidebar-menu">
					<ul>
						<li class="menu-title">
							<span>Main</span>
						</li>
						<li>
							<a asp-page="/Admin/Dashboard/Index"><i class="fe fe-home"></i> <span>Dashboard</span></a>
						</li>
						<li>
							<a asp-page="/Admin/User/Index"><i class="fe fe-user"></i> <span>User</span></a>
						</li>
						<li class="active">
							<a asp-page="/Admin/Coach/Index"><i class="fe fe-users"></i> <span>Coach</span></a>
						</li>
						<li>
							<a asp-page="/Admin/Plan/Index"><i class="fe fe-credit-card"></i> <span>Plan</span></a>
						</li>
						<li>
							<a asp-page="/Admin/Feedback/Index"><i class="fe fe-message-square"></i> <span>Feedback</span></a>
						</li>
						<li>
							<a asp-page="/Admin/Blog/Index"><i class="fe fe-message-square"></i> <span>Blog</span></a>
						</li>
					</ul>
				</div>
			</div>
		</div>
		<!-- /Sidebar -->
		<!-- Page Wrapper -->
		<div class="page-wrapper">
			<div class="content container-fluid">

				<!-- Page Header -->
				<div class="page-header">
					<div class="row">
						<div class="col-sm-12">
							<h3 class="page-title">List of Patient</h3>
						</div>
					</div>
				</div>


				<div class="row">
					<div class="col-sm-12">
						<div class="card">
							<div class="card-body">
								<div class="table-responsive">
									<div class="table-responsive">
										<table class="datatable table table-hover table-center mb-0">
											<thead>
												<tr>
													<th>#</th>
													<th>Mã người huấn luyện</th>
													<th>Họ và tên</th>
													<th>Tiểu sử</th>
													<th>Chuyên môn</th>
													<th>Đánh giá</th>
													<th>Đang sẵn sàng</th>
													<th>Hành động</th>
												</tr>
											</thead>
											<tbody>
												@if (Model.Coaches != null && Model.Coaches.Any())
												{
													foreach (var coach in Model.Coaches)
													{
														<tr>
															<td>@coach.Id</td>
															<td>@coach.UserId</td>
															<td>@coach.FullName</td>
															<td>@coach.Bio</td>
															<td>@coach.Specialization</td>
															<td>@coach.Rating</td>
															<td>@coach.IsAvailable</td>
															<td>
																<form method="post" asp-page-handler="Delete" asp-route-id="@coach.Id"
																	  onsubmit="return confirm('Bạn có chắc chắn muốn xoá người huấn luyện này?');">
																	<button type="submit" class="btn btn-sm btn-danger">Xoá</button>
																</form>
															</td>
														</tr>
													}
												}
												else
												{
													<tr>
														<td colspan="9">Không tìm thấy dữ liệu huấn luyện viên.</td>
													</tr>
												}
											</tbody>
										</table>
									</div>
								</div>
							</div>
						</div>
					</div>
				</div>

			</div>
		</div>
		<!-- /Page Wrapper -->

	</div>
	<!-- /Main Wrapper -->
